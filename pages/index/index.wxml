<view class="card-container">
  <view wx:for="{{pins}}" wx:for-item="pin" wx:key="pin.id" bindtap="onPinClicked"  class="card" style="background-image: url('{{pin.image}}');" data-pin="{{pin}}">


    <view class="card-collected">
      <image class="icon-collected" src="/images/grey/upvote.png">
      </image>
    {{pin.upvotes}}
    </view>
    <view class="card-tags">
      <view class="p">{{pin.tags}}</view>
    </view>
  </view>
</view>



