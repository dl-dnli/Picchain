<view class="page-body">
    <map
      id="myMap"
      style="width: 100%; height: 100vh;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
    ></map>
    <button bindtap="moveToLocation" class="page-body-button" type="primary">移动位置</button>
  </view>
    