<view class="page-body">
    <map
      id="myMap"
      style="width: 100%; height: 100vh;"
      latitude="{{last_location.latitude}}"
      longitude="{{last_location.longitude}}"
      scale="18"
      markers= "{{markers}}"
      show-location
    ></map>
    <button bindtap="moveToLocation" class="page-body-button" type="primary">移动位置</button>
</view>